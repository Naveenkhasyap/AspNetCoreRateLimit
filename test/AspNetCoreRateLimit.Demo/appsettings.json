{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "IpWhitelist": [ "::1/10", "192.168.0.0/24" ],
    "EndpointWhitelist": [ "delete:/api/values", "*:/api/clients", "*:/api/ClientRateLimit", "*:/api/IpRateLimit" ],
    "ClientWhitelist": [ "cl-key-1", "cl-key-2" ],
    "QuotaExceededResponse": {
      "Content": "{{ \"message\": \"Whoa! Calm down, cowboy!\", \"details\": \"Quota exceeded. Maximum allowed: {0} per {1}. Please try again in {2} second(s).\" }}",
      "ContentType": "application/json"
    },
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 2
      },
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 5
      }
    ]
  },

  "IpRateLimitPolicies": {
    "IpRules": [
      {
        "Ip": "84.247.85.224",
        "Rules": [
          {
            "Endpoint": "*",
            "Period": "1s",
            "Limit": 10
          },
          {
            "Endpoint": "*",
            "Period": "1m",
            "Limit": 2
          },
          {
            "Endpoint": "post:/api/values",
            "Period": "1m",
            "Limit": 5
          }
        ]
      },
      {
        "Ip": "84.247.85.225",
        "Rules": [
          {
            "Endpoint": "*",
            "Period": "1s",
            "Limit": 10
          },
          {
            "Endpoint": "*",
            "Period": "1m",
            "Limit": 5
          },
          {
            "Endpoint": "*",
            "Period": "1h",
            "Limit": 2
          }
        ]
      },
      {
        "Ip": "84.247.85.226",
        "Rules": [
          {
            "Endpoint": "*",
            "Period": "1s",
            "Limit": 10
          },
          {
            "Endpoint": "*",
            "Period": "1m",
            "Limit": 5
          },
          {
            "Endpoint": "*",
            "Period": "1d",
            "Limit": 2
          }
        ]
      },
      {
        "Ip": "84.247.85.231",
        "Rules": [
          {
            "Endpoint": "*",
            "Period": "1m",
            "Limit": 0
          }
        ]
      }
    ]
  },

  "ClientRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "EndpointWhitelist": [ "*:/api/values", "delete:/api/clients", "post:/api/clients" ],
    "ClientWhitelist": [ "cl-key-a", "cl-key-b" ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 20
      },
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 50
      },
      {
        "Endpoint": "post:/api/clients",
        "Period": "5m",
        "Limit": 300
      }
    ]
  },

  "ClientRateLimitPolicies": {
    "ClientRules": [
      {
        "ClientId": "cl-key-1",
        "Rules": [
          {
            "Endpoint": "*",
            "Period": "1s",
            "Limit": 10
          },
          {
            "Endpoint": "get:/api/clients",
            "Period": "1m",
            "Limit": 20
          },
          {
            "Endpoint": "put:/api/clients",
            "Period": "5m",
            "Limit": 200
          }
        ]
      },
      {
        "ClientId": "cl-key-2",
        "Rules": [
          {
            "Endpoint": "*",
            "Period": "1s",
            "Limit": 10
          },
          {
            "Endpoint": "get:/api/clients",
            "Period": "1m",
            "Limit": 30
          },
          {
            "Endpoint": "post:/api/clients",
            "Period": "5m",
            "Limit": 50
          }
        ]
      },
      {
        "ClientId": "cl-key-3",
        "Rules": [
          {
            "Endpoint": "post:/api/clients",
            "Period": "1s",
            "Limit": 3
          }
        ]
      }
    ]
  },

  //MP rate limiting policy 
  "MPRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "MPHeader": "X-UIPATH-Metadata",
    "HttpStatusCode": 429,
    "EndpointWhitelist": [ "*:/api/mpvalues", "*:/api/mpratelimit" ],
    "ClientWhitelist": [ "cl-key-a", "cl-key-b" ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 10
      },
      {
        "Endpoint": "*",
        "Period": "3m",
        "Limit": 33
      }
    ]
  },

  "MPRateLimitPolicies": {
    "MPRules": [
      {
        "MpMachineId": "cl-key-1",
        "Rules": [
          {
            "Endpoint": "*",
            "Period": "1s",
            "Limit": 10
          },
          {
            "Endpoint": "*",
            "Period": "6m",
            "Limit": 30
          }
        ]
      }
    ]
  }
}
